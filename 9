#include<stdio.h>
#include<stdlib.h>
struct node
{
	int data;
	struct node* link;
};
void printreverse(struct node*head)
{
	if(head==NULL)
	return;
	printreverse(head->link);
	printf("%d",head->data);
	printf("->");
}
int main()
{
	struct node *head=malloc (sizeof(struct node));
    head->data=4;
    head->link=NULL;

    struct node *current=malloc(sizeof(struct node));
    current ->data=3;
    current->link=NULL;
    head->link=current;

    current=malloc(sizeof(struct node));
    current ->data=2;
    current->link=NULL;
    head->link->link=current;

    printreverse(head);
}
