#include<stdio.h>
int prime(int n);
int main()
{
	int n,i;
	scanf("%d",&n);
	int res=prime(n);
	if(res==1)
	printf("Prime number");
	else
	printf("Not a prime number");
}
int prime(int n)
{
	int i,r,count=0;
	for(i=1;i<=n;i++)
	{
	    if(n%i==0)
	    count+=1;
	}
	if(count==2)
	  return 1;
	else
    return 0;
}
